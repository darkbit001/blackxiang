    <li>
        <div class="WB_feed_spec S_bg5 S_line1 clearfix" action-type="fl_jumpurl" action-data="url=http%3A%2F%2Fhuati.weibo.com%2F19729&target=_blank" exp-data="key=tblog_weibocard&value=1022-topic:ori:3678671690858485:page&src=http://rs.sinajs.cn/j.gif">
            <div class="S_bg4 clearfix">
                                <div class="WB_feed_spec_pic S_bg1">
                                            <a href="http://huati.weibo.com/k/%E8%BE%BE%E4%BA%BA%E7%94%9F%E6%97%A5%E7%A5%9D%E7%A6%8F?from=501"  target="_blank">
